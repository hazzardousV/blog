<div id="header"><h1>Launch</h1></div>
<h1>Articles Tagged with <%= @tag.name %></h1>

<ul>
  <% @tag.articles.each do |article| %>
    <li><%= link_to article.title, article_path(article) %>
    </li>
  <% end %>
</ul>
<%= link_to "edit", edit_tag_path(@tag) %>
<% if logged_in? %>
<%= link_to "delete", tag_path(@tag), method: :delete %>
<% end %>
<%= link_to "<< Back to Tags List", tags_path %>
<div id="footer">
December 23rd,
	Quin Weidner
</div>